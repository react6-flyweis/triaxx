  {
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "VITE_API_BASE_URL": "https://vercel-mr-clement-pos-backend.vercel.app"
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist"
  }